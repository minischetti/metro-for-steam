"resource/layout/screenshotspage_section.layout"
{
	controls
	{
		SectionLabel { controlname="Label" } 
		SectionDivider { controlname="Divider" }
	}
	
	layout  
	{
	  region { name="body" width=max overflow=grow-vertical }
	  
	  place { region=body y=16 control=SectionLabel }
	  place { region=body control=SectionDivider start=SectionLabel height=1 y=8 x=8 width=max }
	  place { region=body y=32 control=*Screenshot dir=right-wrap spacing=8 }
	}
}
